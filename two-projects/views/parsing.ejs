<%- include('partials/header.ejs') %>

    <%- include('partials/navbar.ejs') %>
    <% if(data.img) { %>
        <img src="<%= data.img %>" alt="">
    <% } %>
    <h1><%= data.title %></h1>
    <h3><%= data.description %></h3>
    <h2><%= data.followers %></h2>

    <% for(let posts of data.posts ) { %>
        <article>
            <p><%= posts.title %> - <%= posts.author %></p>
        </article>
    <% } %>
<%- include('partials/footer.ejs') %>